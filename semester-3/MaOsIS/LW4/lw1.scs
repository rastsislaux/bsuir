        <- concept_length;
        <- concept_exact_value;
        => nrel_measurement_in_centimeters: 17;;

    concept_length_4cm
        <- concept_length;
        <- concept_exact_value;
        -> concept_CB;
        => nrel_measurement_in_centimeters: 4;;

    ...
        <- concept_exact_value;
        <- sc_node_class;
        => nrel_measurement_in_centimeters:
            ... (* <- concept_number;; *);;
    
*];;